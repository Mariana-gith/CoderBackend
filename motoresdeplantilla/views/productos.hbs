{{>nav}}
<div class="container">
    <h1>Productos</h1>
    <table class="table">
    <thead>
        <tr>
        <th scope="col">ID</th>
        <th scope="col">NOMBRE</th>
        <th scope="col">PRECIO</th>
        <th scope="col">FOTO</th>
        </tr>
    </thead>
    <tbody>
        {{#each data}}
        <tr>
        <th scope="row">{{this.id}}</th>
        <td>{{this.nombre}}</td>
        <td>{{this.precio}}</td>
        <td><img src={{this.foto}}></td>
        {{/each}}
        </tr>
    </tbody>
    </table>
      <a class="btn btn-outline-success" href="/hbs/form" role="button">Volver a cargar</a>
</div>