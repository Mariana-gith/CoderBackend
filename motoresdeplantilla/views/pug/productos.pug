html 
    head 
        title pug-express 
        link(rel="stylesheet", href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css")
    
    body 
        include ./nav.pug
        h1(id ="text") Productos:
        
    div 
        table(class="table")
            thead 
            th( scope="col") ID
            th( scope="col") NOMBRE
            th( scope="col") PRECIO 
            tbody 
            each pr in data 
                tr 
                th( scope="row")= pr.id
                td( scope="col")= pr.nombre 
                td( scope="col")= pr.precio